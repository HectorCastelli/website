---
import { getBlogPost } from "@lib/blogs";
import Link from "./Link.astro";

interface Props {
  post: string;
}

const { post } = Astro.props;

const blogEntry = await getBlogPost(post);
---

{
  blogEntry ? (
    <Link href={`/blog/${blogEntry?.slug}`}>{blogEntry?.data.title}</Link>
  ) : (
    ""
  )
}
